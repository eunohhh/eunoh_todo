{
    "name": "react_todo",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "deploy": "gh-pages -d dist",
        "json-server": "json-server --watch ./src/data/db.json --port 3001",
        "start": "concurrently \"json-server --watch ./src/data/db.json --port 3001\" \"vite\"",
        "gen-types": "supabase gen types typescript --linked > src/types/supabase.ts"
    },
    "homepage": "https://eunohhh.github.io/eunoh_todo",
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.5",
        "@supabase/supabase-js": "^2.43.2",
        "@types/lodash": "^4.17.1",
        "@types/uuid": "^9.0.8",
        "axios": "^1.6.8",
        "localforage": "^1.10.0",
        "lodash": "^4.17.21",
        "match-sorter": "^6.3.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.23.1",
        "sort-by": "^1.2.0",
        "styled-components": "^6.1.11",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "gh-pages": "^6.1.1",
        "json-server": "^1.0.0-beta.0",
        "typescript": "^5.2.2",
        "vite": "^5.2.0"
    }
}
